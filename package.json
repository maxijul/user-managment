{
    "name": "user-management-yt",
    "version": "1.0.0",
    "type": "module",
    "description": "aplicacion para la gestion de usuarios de una app web",
    "scripts": {
        "dev": "nodemon src/index.js",
        "start": "node src/index.js",
        "lint": "eslint src/**/*.js --fix",
        "prettier": "prettier --write .",
        "prepare": "husky install",
        "postinstall": "npm run prepare"
    },
    "author": "Facundo Villalobo",
    "license": "MIT",
    "devDependencies": {
        "eslint": "8.18.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-config-standard": "17.0.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-n": "15.2.3",
        "eslint-plugin-promise": "6.0.0",
        "husky": "8.0.1",
        "lint-staged": "13.0.2",
        "nodemon": "2.0.18",
        "prettier": "2.7.1"
    },
    "imports": {
        "#Config/*": "./src/config/*",
        "#Controllers/*": "./src/controllers/*",
        "#Dto/*": "./src/dto/*",
        "#Routes/*": "./src/routes/*",
        "#Schemas/*": "./src/schemas/*"
    },
    "lint-staged": {
        "*.js": "eslint --fix",
        "*.{js,md}": "prettier --write"
    },
    "dependencies": {
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "mongoose": "^6.6.1"
    }
}
